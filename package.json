{
	"name": "@zqui/react-terminal",
	"version": "0.0.10",
	"description": "A react component that renders a bash-like terminal.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
  "homepage": "https://react-terminal-demo.onrender.com/",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zaidsidd360/react-terminal.git"
  },
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts --minify --keep-names --external react,react-dom",
		"sourcemap": "tsup src/index.ts --sourcemap"
	},
	"keywords": [
		"terminal",
		"bash",
		"shell",
		"command-line",
		"react-terminal",
		"terminal in react"
	],
	"author": "zaidsidd360",
	"license": "MIT",
	"dependencies": {
		"react": "^18.2.0",
		"react-textarea-autosize": "^8.5.3",
		"use-caret-position": "^0.0.2"
	},
	"devDependencies": {
		"@types/react": "^18.2.31",
		"@types/styled-components": "^5.1.34",
    "styled-components": "^6.1.0",
		"tsup": "^7.2.0",
		"typescript": "^5.4.5"
	},
  "peerDependencies": {
    "react": ">=18.2.0",
    "styled-components": "^6.1.0"
  }
}
